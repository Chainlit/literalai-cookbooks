{
  "name": "audio-summarizer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "predocker:build": "npm i; npm run build",
    "docker:build": "docker build -t dambuty/emojifier .",
    "postdocker:build": "docker push dambuty/emojifier",
    "predocker:run": "[ -f .env.docker ] || echo \"Error: Please create and fill the .env.docker file (without quotes around the values).\"",
    "docker:run": "docker run --env-file .env.docker -p 3001:3000 dambuty/emojifier"
  },
  "dependencies": {
    "@literalai/client": "^0.1.0",
    "clsx": "^2.1.1",
    "form-data": "^4.0.0",
    "mime-types": "^2.1.35",
    "next": "14.2.4",
    "node-loader": "^2.0.0",
    "openai": "^4.50.0",
    "react": "^18",
    "react-audio-voice-recorder": "^2.2.0",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.4",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
